{
  "name": "unified-materials",
  "license": "MIT",
  "private": "true",
  "author": "Dustin Heisey",
  "type": "module",
  "scripts": {
    "format": "prettier . --write",
    "lint:types": "tsc --noEmit",
    "clear": "cd src/main && rimraf resources",
    "lint:scripts": "eslint --flag unstable_native_nodejs_ts_config . --fix",
    "lint": "npm run lint:types && npm run lint:scripts",
    "dev": "cd src/scripts && node --watch ./index.ts",
    "build": "cd src/scripts && node ./index.ts",
    "build:clear": "npm run clear && npm run build",
    "test": "cd src/scripts && node ./test.ts"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "62.7.0",
    "hytale-generators": "^0.2.4",
    "prettier": "3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rimraf": "^6.1.3",
    "type-fest": "^5.4.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
